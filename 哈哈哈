<script>
    // Track basic JavaScript errors
    window.addEventListener('error', function(e) {
        sendAnalytics([
            '_trackEvent',
            'JavaScript Error',
            e.message,
            e.filename + ':  ' + e.lineno
        ].join(' || '), { 'nonInteraction': 1 });
    });

    // Track AJAX errors (jQuery API)
    $(document).ajaxError(function(e, request, settings) {
        sendAnalytics([
            '_trackEvent',
            'Ajax error',
            settings.url,
            e.result
        ].join(' || '), { 'nonInteraction': 1 });
    });
</script>
