<script>
    var exeClickInvite = 0;
    var $isAdmin = +'';
        exeClickInvite = 1;
        $(document).ready(function () {
        //管理员隐藏显示精华
        $('.admin-hide-essence').hideEssence({
            type: 'answer',
        });
        //查看大图没有data-original时会报错
        function srcToriginal(obj) {
            var img = obj.find('img:not()');
            for (var i = 0; i < img.length; i++) {
                var len = img.eq(i).attr('data-original').length;
            }
        }

        $('.comment_area').autoHeight();
        answerVoteScroll();
        var fastawscontent = $('#fastawscontent');
        if (exeClickInvite) {
            var text_holder = $('.text_holder');

            $('.show_full_box').on('click', function () {
                // text_holder.remove();
                fastawscontent.animate({height: '140px'}, 0);
                fastawscontent.css('display', '');
                text_holder.css('display', 'none');
                fastawscontent.focus();
                fastawscontent.blur(function () {
                    if ($(this).val() == '') {
                        $(this).animate({height: '40px'}, 0);
                        // $(this).css('display', 'none');
                        text_holder.css('display', 'block');

                        // $(this).append(text_holder);
                    }
                });
            });
        } else {
            $('#fastawscontent').focus(function () {
                $(this).animate({height: '140px'}, 150, 'linear', function () {
                    $(window).trigger('scroll');
                });
                $('.text_holder').remove();
            });
            $('#fastawscontent').blur(function () {
                if ($(this).val() == '') {
                    $(this).animate({height: '40px'}, 150, 'linear', function () {
                        $(window).trigger('scroll');
                    });
                }
            });
        }

        function showTips() {
            function showTip(obj) {
                var style = 'position:absolute;padding:5px 10px;color:#fff;border-radius:3px;background:#444;background:rgba(0,0,0,0.7);';
                var style_arrow = 'left: 10px;position: absolute;width: 0;height: 0;border-left: 6px solid transparent;border-right: 6px solid transparent;';
                var tipDiv = $('#tipDiv')[0];
                var tip_con = obj.attr('title-tip');
                var obj_top = obj.offset().top;
                var obj_left = obj.offset().left;
                var obj_height = obj.outerHeight();
                if (tipDiv) {
                    tipDiv.style.display = 'block';
                    var arrow = tipDiv.firstChild;
                } else {
                    tipDiv = document.createElement('div');
                    var arrow = document.createElement('div');
                    var span = document.createElement('span');
                    tipDiv.id = 'tipDiv';
                    tipDiv.appendChild(arrow);
                    tipDiv.appendChild(span);
                    document.body.appendChild(tipDiv);
                }
                if (obj.hasClass('vote_down')) {
                    arrow.style.cssText = style_arrow + 'top: -6px;border-bottom: 6px solid #444;border-bottom: 6px solid rgba(0,0,0,0.7);';
                    tipDiv.style.cssText = style + 'top:' + parseInt(obj_top + obj_height + 8) + 'px;' + 'left:' + obj_left + 'px;';
                } else {
                    arrow.style.cssText = style_arrow + 'bottom: -6px;border-top: 6px solid #444;border-top: 6px solid rgba(0,0,0,0.7);';
                    tipDiv.style.cssText = style + 'top:' + (obj_top - 38) + 'px;' + 'left:' + obj_left + 'px;';
                }
                //console.log(tipDiv.innerText);
                if (tipDiv.innerText == undefined) { //firefox
                    tipDiv.lastChild.textContent = tip_con;
                } else {
                    tipDiv.lastChild.innerText = tip_con;
                }
            }

            function hideTip() {
                var tipDiv = $('#tipDiv');
                if (tipDiv.length) {
                    tipDiv.hide();
                }
            }

            var otips = $('[title-tip]');
            otips.hover(function () {
                showTip($(this));
            }, function () {
                hideTip();
            });
        }

        showTips();
        if ($('#all_answer .answer_content').length > 0) {
            $('#all_answer .answer_content').viewer({
                url: function (e) {
                    if (e.getAttribute('data-original') == undefined) {
                        return e.getAttribute('src').replace(/.thumb.jpg/g, '').replace(/-700/g, '');
                    } else {
                        return e.getAttribute('data-original').replace(/.thumb.jpg/g, '').replace(/-700/g, '');
                    }
                },
                toolbar: 2,
                title: false

            });
        }

        $('.sidebar_block .link_bbtn').clone().appendTo($('.answer-top .g-side'));
        $('.answer-top').find('a[action-type=q_fave]').unbind('click').click(function () {
            add_qt_fave($(this), 'question')
        });
        $('.answer-top h3').click(function () {
            $('html,body').animate({scrollTop: '0px'}, 500);
        });
        listenWindowScroll(function () {
            // console.log($(window).scrollTop(), $('#qdata').offset().top);
            if ($(window).scrollTop() > $('#qdata').offset().top) {
                $('.answer-top').css('top', 0);
            } else {
                $('.answer-top').css('top', '-100%');
            }
        });


        // 关注/取消关注用户
        $('.usr-favor').off('click').on('click', function () {
            var that = $(this),
                type = that.data('action'),
                uid = that.data('uid');
            $.ajax({
                type: 'POST',
                url: '/wenda/answer/follow.html',
                data: {
                    type: type,
                    fuid: uid
                },
                dataType: 'json',
                success: function (res) {
                    var num = parseInt($('.follow-number').text());
                    // console.log(res);
                    if (res['status'] === 1) {
                        if (type === 'add') {
                            that.text('已关注');
                            $('.follow-number').text(num + 1);
                            that.data('action', 'del');
                        } else {
                            that.text('关注');
                            $('.follow-number').text(num - 1);
                            that.data('action', 'add');
                        }
                        that.toggleClass('selected');
                    } else {
                        if (res['msg'] === 'login') {
                            showLogin();
                        } else {
                            dialogShowMessage(res['msg'], true, 1500);
                        }
                    }
                }
            });
        });

        // 相关问题悬浮在右侧
        $('.related-wrap').stick_in_parent({
            parent: '.container',
            offset_top: 70
        });

        // 绑定图片查看器
        $('.q_show_content').viewer({
            url: function (e) {
                if (e.getAttribute('data-original') == undefined) {
                    return e.getAttribute('src').replace(/.thumb.jpg/g, '').replace(/-700/g, '');
                } else {
                    return e.getAttribute('data-original').replace(/.thumb.jpg/g, '').replace(/-700/g, '');
                }
            },
            toolbar: 2,
            title: false
        });

    });
</script>

